{
  "compilerOptions": {
    "outDir": "./build",
    "module": "commonjs",
    "strict": true,
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "target": "esnext",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "types": [
      "react",
      "react-native"
    ]
  },
  "exclude": [
    "node_modules",
    "build"
  ]
}
